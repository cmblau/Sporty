<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name="android.support.multidex.MultiDexApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".google.firebase.quickstart.auth.ChooserActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		
		<activity android:name=".gridview.FlowerGalleryActivity">

		</activity>
		<activity android:name=".example.cem.sporty.MainActivity">


		</activity>
		
        <activity
            android:name=".google.firebase.quickstart.auth.CustomAuthActivity"
            android:label="@string/label_custom_auth" />
        <activity
            android:name=".google.firebase.quickstart.auth.GoogleSignInActivity"
            android:label="@string/label_google_sign_in" />
        <activity
            android:name=".google.firebase.quickstart.auth.FacebookLoginActivity"
            android:label="@string/label_facebook_login" />
        <activity
            android:name=".google.firebase.quickstart.auth.EmailPasswordActivity"
            android:label="@string/label_emailpassword"
